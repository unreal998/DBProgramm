<% include ../partials/header %>
<br>
<form action="http://localhost:4000/BDiZ/guarantee/edit/<%= item.list %>" method="post" class="col s12">
	<input type="hidden" name="id_guarantee" value="">
	<div class="form-group row">
		<label for="list" class="col-2 col-form-label">Номер накладної</label>
		<div class="col-10">
			<input type="text" name="list" value="<%= item.list %>" class="form-control">
		</div>
	</div>
	<div class="form-group row">
		<label for="productCode" class = "col-2 col-form-label">Код продукту</label>
			<div class="col-10">
				<input type="text" name="productCode" value="<%= item.productCode %>" class="form-control">
			</div>
	</div>
		<div class="form-group row">
		<label for="endGuarantee" class = "col-2 col-form-label">Дата кінця гарантії</label>
			<div class="col-10">
				<input type="date" name="endGuarantee" value="<%= item.endGuarantee %>" class="form-control">
			</div>
	</div>
	<div class="form-group row">
		<label for="typeOfGuaranteeWork	" class = "col-2 col-form-label">Тип гарантійних робіт</label>
			<div class="col-10">
				<input type="text" name="typeOfGuaranteeWork" value="<%= item.typeOfGuaranteeWork	 %>" class="form-control">
			</div>
	</div>
	<div class="form-group row">
		<div class="col-10">
			<input type="submit" name="submit" value="Зберегти" class="btn btn-primary btn-lg">
		</div>
	</div>
</form>
<% include ../partials/footer %>