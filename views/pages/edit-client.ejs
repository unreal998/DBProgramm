<% include ../partials/header %>
<br>
<form action="http://localhost:4000/BDiZ/client/edit/<%= item.list %>" method="post" class="col s12">
	<input type="hidden" name="id_client" value="">
	<div class="form-group row">
		<label for="name" class="col-2 col-form-label">ПІБ</label>
		<div class="col-10">
			<input type="text" name="name" value="<%= item.name %>" class="form-control">
		</div>
	</div>
	<div class="form-group row">
		<label for="list" class = "col-2 col-form-label">Номер накладної</label>
			<div class="col-10">
				<input type="text" name="list" value="<%= item.list %>" class="form-control">
			</div>
	</div>
	<div class="form-group row">
		<label for="phone" class = "col-2 col-form-label">Номер телефона</label>
			<div class="col-10">
				<input type="text" name="phone" value="<%= item.phone %>" class="form-control">
			</div>
	</div>
	<div class="form-group row">
		<label for="often" class = "col-2 col-form-label">Постійний</label>
			<div class="col-10">
				<input type="text" name="often" value="<%= item.often %>" class="form-control">
			</div>
	</div>
	<div class="form-group row">
		<div class="col-10">
			<input type="submit" name="submit" value="Зберегти" class="btn btn-primary btn-lg">
		</div>
	</div>
</form>
<% include ../partials/footer %>